{
  "name": "fixup-twitter-link",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "node .",
    "dev": "node -r dotenv/config . dotenv_config_override=true",
    "build": "swc src --only **/*.ts --out-dir dist",
    "build:debug": "swc --source-maps=true src --out-dir=dist",
    "type:check": "tsc --noEmit",
    "format": "dprint fmt",
    "format:check": "dprint check"
  },
  "license": "MIT",
  "packageManager": "pnpm@8.9.1",
  "engines": {
    "npm": "This project is configured to use pnpm"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.93",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20.8.6",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "dotenv": "16.3.1",
    "dprint": "0.41.0",
    "eslint": "8.51.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@discordjs/core": "1.0.1",
    "@discordjs/rest": "2.0.1",
    "@discordjs/ws": "1.0.1"
  },
  "private": true
}
